<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <settings defaultResolver="main" />
  <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it.-->
  <credentials host="artifactorypoc.eu-gb.mybluemix.net" realm="Artifactory Realm" username="admin" passwd="password" />
  <resolvers>
    <chain name="main">
      <url name="public" m2compatible="false root=http://artifactorypoc.eu-gb.mybluemix.net/artifactory">
        <artifact pattern="http://artifactorypoc.eu-gb.mybluemix.net/artifactory/Unit1/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
        <ivy pattern="http://artifactorypoc.eu-gb.mybluemix.net/artifactory/Unit1/[organisation]/[module]/ivy-[revision].xml" />
      </url>
    </chain>
  </resolvers>
</ivy-settings>
